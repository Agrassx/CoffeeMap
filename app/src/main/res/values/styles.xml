<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">

    </style>

    <style name="menu_labels_style">
        <item name="android:background">@drawable/fab_label_background</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="whiteRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/white_ratingbar</item>
        <item name="android:minHeight">24dp</item>
        <item name="android:maxHeight">24dp</item>
        <item name="android:numStars">5</item>
    </style>

    <style name="RatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:minHeight">24dip</item>
        <item name="android:maxHeight">24dip</item>
        <item name="colorControlNormal">@color/white</item>
        <item name="colorControlActivated">@color/white_pressed</item>
    </style>
</resources>